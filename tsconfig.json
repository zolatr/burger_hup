{
    "compilerOptions": {
        "target": "ES2020",
        "useDefineForClassFields": true,
        "lib": [
            "ES2020",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "skipLibCheck": true,
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@admin/*": [
                "./src/modules/admin/*"
            ],
            "@menu/*": [
                "./src/modules/menu/*"
            ],
            "@kitchen/*": [
                "./src/modules/kitchen/*"
            ],
            "@home/*": [
                "./src/modules/home/*"
            ],
            "@shared/*": [
                "./src/shared/*"
            ],
            "@store/*": [
                "./src/store/*"
            ]
        }
    },
    "include": [
        "src"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}